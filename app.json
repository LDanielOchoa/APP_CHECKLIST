{
  "expo": {
    "name": "AppExpo",
    "slug": "AppExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.AppExpo"
    },
    "web": {
      "favicon": "./assets/imagenes/logo.png"
    },
    "plugins": [
      "expo-asset"
    ],
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "5abf73af-fa80-468a-856c-4d03778ffa9c"
      }
    }
  }
}
